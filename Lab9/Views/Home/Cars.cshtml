@{
    ViewBag.Title = "Cars";
}
<h2>@ViewBag.Title.</h2>

@model List<Lab9.Models.cars>
<div class="container">
    <div class="row">
        <table class="table">
            <tr>
                <th>ID</th>
                <th>Name</th>
            </tr>

            @foreach (var car in Model)
            {
                <tr>
                    <td>
                        @Html.DisplayFor(_ => car.id)
                    </td>
                    <td>
                        @Html.DisplayFor(_ => car.name)
                    </td>
                </tr>
            }
        </table>
    </div>
</div>
